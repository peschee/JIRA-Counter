{
	"name": "JIRA Story Points Counter",
	"version": "1.0",
	"description": "Counts the number of story points in JIRA filters.",

	"content_scripts": [{
		"matches": ["https://jira.liip.ch/*"],
		"js": ["jquery.min.js", "jira.js"],
		"run_at": "document_end"
	}],

	"icons": {
		"16": "icon_16.png",
		"32": "icon_32.png",
		"64": "icon_64.png",
		"128": "icon_128.png"
	}
}