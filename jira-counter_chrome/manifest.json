{
	"name": "JIRA Counter",
	"version": "1.0.4",
	"description": "Counts the number of story points (and some other things) in JIRA filters.",

	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["jquery.min.js", "jira.js"],
		"run_at": "document_end"
	}],

	"icons": {
		"16": "icon_16.png",
		"32": "icon_32.png",
		"64": "icon_64.png",
		"128": "icon_128.png"
	}
}
